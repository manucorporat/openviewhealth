{
  "name": "open-view-health",
  "private": true,
  "version": "0.0.1",
  "description": "Open View Health is a free app anyone can use to visualize and securely share their medical data. A modern DICOM visualizer powered by Sethealth.",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "fmt": "prettier --write src",
    "fmt.check": "prettier --check src"
  },
  "devDependencies": {
    "@sethealth/core": "0.8.2",
    "@stencil/core": "^2.5.0",
    "@stencil/store": "^1.4.1",
    "copy-text-to-clipboard": "^2.2.0",
    "prettier": "^2.2.1",
    "workbox-build": "^4.3.1"
  },
  "license": "MIT"
}
