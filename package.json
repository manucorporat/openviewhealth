{
  "name": "open-view-health",
  "private": true,
  "version": "0.0.1",
  "description": "OpenView Health is a free app to visualize and securely share medical data. A modern open source DICOM visualizer.",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "fmt": "prettier --write src",
    "fmt.check": "prettier --check src"
  },
  "devDependencies": {
    "@sethealth/core": "0.11.1",
    "@stencil/core": "2.8.1",
    "@stencil/store": "^1.4.1",
    "copy-text-to-clipboard": "3.0.1",
    "prettier": "2.4.1",
    "workbox-build": "^4.3.1"
  },
  "license": "MIT"
}
